{{! Main page title and subtitle !}}
    <header class="page content">
        <!--googleoff: index-->
        <nav id="mobile-menu"></nav>
        <!--googleon: index-->
        <div id="top-bar">
            <h1>{{ title }}</h1>
        {{~#if !sub_title.is_empty() }}
            <h4>{{ sub_title }}</h4>
        {{~/if }}
            <div id="mobile-menu-button">{{{ html.icon("menu") }}}</div>
            {{~> @partial-block }}
        </div>
    </header>
    {{> side_menu }}